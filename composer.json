{
  "name": "homer/alipay",
  "version": "1.0.0",
  "description": "Alipay support",
  "license": "proprietary",
  "type": "library",

  "autoload": {
    "files": [
      "src/Support/helpers.php"
    ],
    "psr-4": {
      "Homer\\Payment\\Alipay\\": "src/"
    }
  },

  "autoload-dev": {
    "files": [
      "spec/Support/assert.php"
    ]
  },

  "config": {
    "preferred-install": "dist"
  },

  "minimum-stability": "dev",
  "prefer-stable": true,

  "require": {
    "ext-mbstring": "*",
    "guzzlehttp/guzzle": "^6.1"
  },

  "require-dev": {
    "phpspec/phpspec": "^2.5"
  }
}
